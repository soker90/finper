{
  "name": "@soker90/finper-api",
  "version": "1.3.0",
  "description": "Finper API that stores endpoints consumed by a Finper client",
  "main": "src/server",
  "scripts": {
    "start": "npm run build-ts && npm run serve",
    "start:pro": "LOG=1 NODE_ENV=prod npm run serve",
    "start:dev": "NODE_ENV=dev npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/server.js",
    "build-ts": "tsc",
    "test": "NODE_ENV=test jest --maxWorkers=50% --silent",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soker90/finper.git"
  },
  "keywords": [
    "Replit",
    "api",
    "rest"
  ],
  "author": "Eduardo Parra Mazuecos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soker90/finper/issues"
  },
  "homepage": "https://github.com/soker90/finper#readme",
  "engines": {
    "node": "22.x"
  },
  "dependencies": {
    "@hapi/boom": "10.0.1",
    "@soker90/finper-models": "1.3.0",
    "bcrypt": "6.0.0",
    "bluebird": "3.7.2",
    "compression": "1.8.1",
    "cors": "2.8.5",
    "dotenv": "17.2.1",
    "express": "^5.1.0",
    "helmet": "8.1.0",
    "joi": "18.0.0",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.1",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "uuid": "11.1.0",
    "winston": "3.17.0",
    "winston-loki": "6.1.3",
    "winston-transport": "4.9.0"
  },
  "devDependencies": {
    "@faker-js/faker": "10.0.0",
    "@shelf/jest-mongodb": "5.2.2",
    "@tsconfig/node22": "22.0.2",
    "@types/bcrypt": "6.0.0",
    "@types/bluebird-global": "3.5.18",
    "@types/compression": "1.7.5",
    "@types/cors": "2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "30.0.0",
    "@types/jsonwebtoken": "9.0.1",
    "@types/morgan": "1.9.10",
    "@types/node": "24.3.0",
    "@types/passport": "1.0.17",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@types/supertest": "6.0.3",
    "@types/uuid": "10.0.0",
    "eslint": "9.33.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.0.1",
    "jest": "30.0.5",
    "neostandard": "^0.12.2",
    "nock": "14.0.10",
    "supertest": "7.1.4",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  }
}
