{
  "name": "@soker90/finper-api",
  "version": "0.1.0",
  "description": "Replit API that stores endpoints consumed by a Replit client",
  "main": "src/server",
  "scripts": {
    "start": "npm run build-ts && npm run serve",
    "start:pro": "LOG=1 NODE_ENV=prod npm run serve",
    "start:dev": "NODE_ENV=dev npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/server.js",
    "build-ts": "tsc",
    "test": "NODE_ENV=test jest --maxWorkers=50% --silent",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soker90/finper.git"
  },
  "keywords": [
    "Replit",
    "api",
    "rest"
  ],
  "author": "Replit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soker90/finper/issues"
  },
  "homepage": "https://github.com/soker90/finper#readme",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "@soker90/finper-models": "^0.0.8",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.0.2",
    "joi": "^17.6.0",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "ts-node": "^10.8.2",
    "uuid": "^8.3.2",
    "winston": "^3.8.1",
    "winston-loki": "^6.0.5",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.1.0",
    "@shelf/jest-mongodb": "^3.0.2",
    "@tsconfig/node16": "^1.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird-global": "^3.5.13",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.1",
    "@types/passport": "^1.0.8",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.0",
    "nock": "^13.2.8",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3"
  }
}
