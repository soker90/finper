{
  "name": "@soker90/finper-api",
  "version": "1.1.1",
  "description": "Finper API that stores endpoints consumed by a Finper client",
  "main": "src/server",
  "scripts": {
    "start": "npm run build-ts && npm run serve",
    "start:pro": "LOG=1 NODE_ENV=prod npm run serve",
    "start:dev": "NODE_ENV=dev npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/server.js",
    "build-ts": "tsc",
    "test": "NODE_ENV=test jest --maxWorkers=50% --silent",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soker90/finper.git"
  },
  "keywords": [
    "Replit",
    "api",
    "rest"
  ],
  "author": "Eduardo Parra Mazuecos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soker90/finper/issues"
  },
  "homepage": "https://github.com/soker90/finper#readme",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@soker90/finper-models": "^1.2.0",
    "bcrypt": "^5.1.0",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "joi": "^17.9.0",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-loki": "^6.0.6",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.1",
    "@shelf/jest-mongodb": "^4.1.7",
    "@tsconfig/node16": "^16.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird-global": "^3.5.13",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.1.0",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.36.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "nock": "^13.3.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  }
}
