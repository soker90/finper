{
  "name": "@soker90/finper-api",
  "version": "1.4.7",
  "description": "Finper API that stores endpoints consumed by a Finper client",
  "main": "src/server",
  "scripts": {
    "start": "pnpm run build-ts && pnpm run serve",
    "start:pro": "LOG=1 NODE_ENV=prod pnpm run serve",
    "start:dev": "NODE_ENV=dev pnpm run serve",
    "build": "pnpm run build-ts",
    "serve": "node dist/server.js",
    "build-ts": "tsc",
    "test": "NODE_ENV=test jest --maxWorkers=50% --silent",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soker90/finper.git"
  },
  "keywords": [
    "Replit",
    "api",
    "rest"
  ],
  "author": "Eduardo Parra Mazuecos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soker90/finper/issues"
  },
  "homepage": "https://github.com/soker90/finper#readme",
  "engines": {
    "node": ">=24.x"
  },
  "dependencies": {
    "@hapi/boom": "10.0.1",
    "@soker90/finper-models": "1.3.0",
    "bcrypt": "6.0.0",
    "bluebird": "3.7.2",
    "compression": "1.8.1",
    "cors": "2.8.6",
    "dotenv": "17.3.1",
    "express": "^5.1.0",
    "helmet": "8.1.0",
    "joi": "18.0.2",
    "jsonwebtoken": "9.0.3",
    "morgan": "1.10.1",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "uuid": "13.0.0",
    "winston": "3.19.0",
    "winston-loki": "6.1.4",
    "winston-transport": "4.9.0"
  },
  "devDependencies": {
    "@faker-js/faker": "9.9.0",
    "@shelf/jest-mongodb": "6.0.2",
    "@tsconfig/node22": "22.0.5",
    "@types/bcrypt": "6.0.0",
    "@types/bluebird-global": "3.5.18",
    "@types/compression": "1.8.1",
    "@types/cors": "2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "30.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/morgan": "1.9.10",
    "@types/node": "25.2.3",
    "@types/passport": "1.0.17",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@types/supertest": "6.0.3",
    "@types/uuid": "11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.15.0",
    "globals": "^17.3.0",
    "jest": "30.2.0",
    "neostandard": "^0.12.2",
    "nock": "14.0.11",
    "supertest": "7.2.2",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.55.0"
  }
}
